<div id='header'>
  {{view 'select'
    content=conventions
    optionValuePath="content.id"
    optionLabelPath="content.name"
    value=activeConvention
    prompt="Select Con..."}}

  <div id='sidebar_toggle'>
    {{fa-icon 'bars'}}
  </div>
  <div id='search'>
    {{input value=search classNames='search' placeholder='Search'}}
  </div>
</div>

{{outlet}}
{{outlet 'modal'}}
