<g class="svg-pan-zoom_viewport">
  <image {{bind-attr xlink:href=model.image}} x="0" y="0" width="600px" height="400px"/>

  {{#each area in model.mapAreas}}
    {{map-area
      model=area
      selectedArea=selectedArea
      action='selectArea'
      }}
  {{/each}}

  {{#each point in tempPoints}}
    <circle class='temp'
      {{bind-attr cx='point.firstObject'}}
      {{bind-attr cy='point.lastObject'}}
      {{bind-attr r='dotRadiusScaled'}}
    />
  {{/each}}

  <polyline class='temp' {{bind-attr points='tempLine'}} />
</g>
